<%- include('include/header') -%>
    <div class="site-tags">
      <h2><%= app.category.name %><span> - <%- app.i18n.translatable('Category') %></span></h2>
      <ul>
        <% app.list.forEach(post => { %><li>
          <span class="date"><%= app.moment(post.publishedAt).format('YYYY-MM-DD') %></span>
          <a href="<%= app.url.post(post) %>"><%= post.title %></a>
        </li><% }) %>
      </ul>
    </div>
    <%- include('include/paging', {
      pagingUrl (page = 0) {
        return app.url.categories(app.category.slug, page)
      }
    }) -%>
<%- include('include/footer') -%>
