  <%_ app.config.inject.body -%>
  <script src="<%= app.url.static('js/app.js') %>"></script>
  <% if (app.config.theme.config.gaId) { %>
  <script id="gtag-script" async src="https://www.googletagmanager.com/gtag/js?id=<%= app.config.theme.config.gaId %>" data-gtag="<%= app.config.theme.config.gaId %>"></script>
  <% } %>
</body>
</html>
